{
	"name": "@nan0web/xml",
	"version": "1.0.1",
	"description": "XML transformer and utilities.",
	"type": "module",
	"main": "./src/index.js",
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"types": "./types/index.d.ts",
			"import": "./src/index.js"
		}
	},
	"files": [
		"src/**/*.js",
		"!src/**/*.test.js",
		"types/**/*.d.ts"
	],
	"keywords": [
		"nano",
		"nan0",
		"nan0web",
		"xml",
		"transformer"
	],
	"scripts": {
		"build": "tsc",
		"test": "node --test \"src/**/*.test.js\"",
		"test:coverage": "node --experimental-test-coverage --test-coverage-include=\"src/**/*.js\" --test-coverage-exclude=\"src/**/*.test.js\" --test \"src/**/*.test.js\"",
		"test:coverage:collect": "nan0test coverage",
		"test:docs": "node --test src/README.md.js",
		"test:release": "node --test \"releases/**/*.test.js\"",
		"test:status": "nan0test status --hide-name --debug",
		"play": "node play/main.js",
		"precommit": "npm test",
		"prepush": "npm test",
		"prepare": "husky",
		"release": "nan0release publish",
		"clean": "rm -rf .cache/ && rm -rf dist/",
		"clean:modules": "rm -rf node_modules"
	},
	"author": "ЯRаСлав YaRaSLove <support@yaro.page>",
	"license": "ISC",
	"devDependencies": {
		"@nan0web/db": "^1.0.2",
		"@nan0web/db-fs": "^1.0.0",
		"@nan0web/log": "^1.0.0",
		"@nan0web/transformer": "^1.0.0",
		"@nan0web/types": "^1.0.2",
		"@nan0web/ui-cli": "^1.0.0"
	},
	"peerDependencies": {
		"@nan0web/db": "^1.0.0",
		"@nan0web/db-fs": "^1.0.0",
		"@nan0web/log": "^1.0.0",
		"@nan0web/transformer": "^1.0.0",
		"@nan0web/types": "^1.0.0"
	}
}
